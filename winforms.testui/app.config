<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>

  <connectionStrings>
    <add name="Default" connectionString="Server=localhost;Port=5433;Database=JalapenoCloud;User Id=jalapeno;Password=jalapeno241187;Protocol=3;Pooling=true;MinPoolSize=1;MaxPoolSize=20;Timeout=20;ConnectionLifeTime=60;CommandTimeout=60;SSL=false;SslMode=Disable;"/>
  </connectionStrings>

  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets>
      <target xsi:type="File" name="errors" fileName="c:/Test/nlogs/${shortdate}.log"
              layout="${longdate} [${level}] [${threadid}] ${exception:format=ToString} ${message} ${newline}" />


    </targets>

    <rules>

      <logger name="*" minlevel="Debug" writeTo="errors" />

    </rules>
  </nlog>

  <appSettings>
  </appSettings>

</configuration>
